{
  "$defs": {
    "NotExistType": {
      "const": "<NotExist>",
      "type": "string"
    },
    "RegistryDefinition": {
      "properties": {
        "path": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/RegistryValueKind"
        },
        "old_value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "$ref": "#/$defs/NotExistType"
            }
          ]
        },
        "new_value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "$ref": "#/$defs/NotExistType"
            }
          ]
        }
      },
      "required": ["path", "name", "type", "old_value", "new_value"],
      "type": "object"
    },
    "RegistryValueKind": {
      "enum": [
        "String",
        "ExpandString",
        "Binary",
        "DWord",
        "MultiString",
        "QWord"
      ],
      "type": "string"
    },
    "SchtaskDefinition": {
      "properties": {
        "full_path": {
          "type": "string"
        },
        "old_state": {
          "$ref": "#/$defs/SchtaskState"
        },
        "new_state": {
          "$ref": "#/$defs/SchtaskState"
        }
      },
      "required": ["full_path", "old_state", "new_state"],
      "type": "object"
    },
    "SchtaskState": {
      "enum": ["Enabled", "Disabled"],
      "type": "string"
    },
    "ScriptDefinition": {
      "properties": {
        "apply": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "revert": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": ["apply", "revert"],
      "type": "object"
    },
    "ServiceDefinition": {
      "properties": {
        "name": {
          "type": "string"
        },
        "old_startup": {
          "$ref": "#/$defs/ServiceStartupType"
        },
        "new_startup": {
          "$ref": "#/$defs/ServiceStartupType"
        }
      },
      "required": ["name", "old_startup", "new_startup"],
      "type": "object"
    },
    "ServiceStartupType": {
      "enum": [
        "Automatic",
        "AutomaticDelayedStart",
        "Disabled",
        "InvalidValue",
        "Manual"
      ],
      "type": "string"
    },
    "TaskDefinition": {
      "additionalProperties": false,
      "anyOf": [
        {
          "required": ["registries"]
        },
        {
          "required": ["scheduled_tasks"]
        },
        {
          "required": ["services"]
        },
        {
          "required": ["script"]
        }
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "registries": {
          "default": [],
          "items": {
            "$ref": "#/$defs/RegistryDefinition"
          },
          "type": "array"
        },
        "scheduled_tasks": {
          "default": [],
          "items": {
            "$ref": "#/$defs/SchtaskDefinition"
          },
          "type": "array"
        },
        "services": {
          "default": [],
          "items": {
            "$ref": "#/$defs/ServiceDefinition"
          },
          "type": "array"
        },
        "script": {
          "$ref": "#/$defs/ScriptDefinition",
          "default": {
            "apply": null,
            "revert": null
          }
        }
      },
      "required": ["id", "name", "description"],
      "type": "object"
    }
  },
  "properties": {
    "task_definitions": {
      "default": [],
      "items": {
        "$ref": "#/$defs/TaskDefinition"
      },
      "type": "array"
    },
    "preload": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    }
  },
  "type": "object"
}
